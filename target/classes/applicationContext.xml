<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd" >

	<bean id="setterBean" class="com.shapestone.spring_ioc.Student" scope="prototype">
		<property name="name" value="Shapestone"></property>
	</bean>
	
	<bean id="constructorBean" class="com.shapestone.spring_ioc.constructor.Student">
		<constructor-arg value="Shapestone" type = "String"></constructor-arg>
		<constructor-arg value="10" type = "int"></constructor-arg>
		
	</bean>

</beans>  